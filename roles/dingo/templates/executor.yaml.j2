cluster:
    name: dingo
exchange:
    host: {{ inventory_hostname }}
    port: {{ dingo_executor_exchange_port }}
server:
    coordinators: {{ dingo_coordinator_exchange_connection_list }}
    user: user
    keyring: TO_BE_CONTINUED
    resourceTag: 1
    mysqlPort: {{ dingo_mysql_port }}
variable:
    autoIncrementCacheCount: {{ dingo_auto_increment_cache_count }}
    autoIncrementIncrement: 1
    autoIncrementOffset: 1
    enableTableLock: true
    lowerCaseTableNames: 2
common:
    scheduledCoreThreads: 16
    lockCoreThreads: 0
    globalCoreThreads: 0
    gcSafePointPeriod: 300
    gcDeleteRegionPeriod: 60
store:
    bufferSize: {{ dingo_executor_buffer_size }}
    bufferNumber: {{ dingo_executor_buffer_number }}
    fileSize: {{ dingo_executor_file_size }}
    path: {{ dingo_home }}/localStore
security:
    ldap:
        ldapHost: {{ openldap_server_ip }}
        ldapPort: {{ openldap_server_port }}
        bindDN: {{ openldap_server_bindDN }}
        password: {{ openldap_server_root_password }}
        baseDN: {{ openldap_server_baseDN }}
    cipher:
        keyPath: {{ dingo_home }}/conf/dingodb.jks
        keyPass: dingodb
        storePass: dingodb
        alias: dingodb
        issuer: dingo